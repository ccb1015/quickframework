<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
	"http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
  <package name="genController" namespace="/" extends="struts-default">
  	<action name="genController!*" class="com.bonc.quickframework.web.action.GenControllerAction" method="{1}">
  		<result name="list">/WEB-INF/pages/GenController/list.jsp</result>
  		<result name="add">/WEB-INF/pages/GenController/edit.jsp</result>
  		<result name="edit">/WEB-INF/pages/GenController/edit.jsp</result>
  		<result name="success" type="redirectAction">genController!list</result>
  	</action>
  </package>
</struts>	